abils=open(r"C:\Users\scien\Documents\GitHub\Orange-Peeler\abiltext")
abil=abils.readlines()
jsons=open(r"C:\Users\scien\Documents\GitHub\Orange-Peeler\ability-data","r+")
jsons.write("{")
for item in abil:
    seq=item.split("|")
    jsons.write('\t"'+seq[1]+'":{\n')
    jsons.write('\t\t"id":"'+seq[0]+'",\n')
    jsons.write('\t\t"detail":"'+seq[2]+'",\n')
    jsons.write('\t\t"gen":"'+seq[3][0:-1]+'"},\n')
jsons.write("}")