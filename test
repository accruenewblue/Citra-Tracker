file3=open(r"C:\Users\scien\Documents\GitHub\Orange-Peeler\test4","r")
file3=open(r"C:\Users\scien\Documents\GitHub\Orange-Peeler\test4","r")
ext2=file3.readlines()
def insertion_sort(list1): 
        for i in range(1, len(list1)): 
            a = list1[i] 
            j = i - 1 
            while j >= 0 and a < list1[j]: 
                list1[j + 1] = list1[j] 
                j -= 1     
            list1[j + 1] = a 
        return list1 
for item in insertion_sort(ext2):
    print((item.split("|")))